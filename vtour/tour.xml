<action name="oculta_layer">
	set (layer[intro].visible,false);
	
</action>
 <layer name="intro" 
			url="intro.png"
			keep="true"
			
			scale="0.1"
			x="10"
			y="160"
			edge="center"
			align="center"
			onclick="oculta_layer();"
		zorder="9999"
		enabled="true"
		bgcapture="true"
		visible="true"
	        />
	
